{
    "name": "blazervel/bladebox",
    "description": "Component documentation package for Laravel Blade Components.",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Anderton",
            "email": "hey@joshuaanderton.ca"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.67|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Blazervel\\Bladebox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blazervel\\Bladebox\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blazervel\\Bladebox\\Providers\\BladeboxServiceProvider"
            ]
        }
    }
  }